{
  "name": "overlay",
  "productName": "overlay",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "clean": "rm -rf .cache",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "make:all": "electron-forge make --platform=linux && electron-forge make --platform=win32 && electron-forge make --platform=darwin && yarn clean",
    "lint": "node_modules/.bin/eslint src/",
    "test": "node_modules/mocha/bin/mocha --require @babel/register --require @babel/polyfill 'test/**/*.@(js|jsx)'"
  },
  "keywords": [],
  "author": {
    "name": "dlittig",
    "email": "david0605.littig@hotmail.de"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "overlay"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-async-generator-functions": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^3.0.0",
    "electron": "8.2.0",
    "electron-mocha": "^8.2.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^6.0.0",
    "node-loader": "^0.6.0",
    "prop-types": "^15.7.2",
    "spectron": "^10.0.1",
    "style-loader": "^1.1.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.9.0",
    "react-jss": "^10.0.4",
    "react-notification-system-redux": "^2.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "url-parse": "^1.4.7"
  }
}
